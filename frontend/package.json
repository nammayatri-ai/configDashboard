{
  "name": "config-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:master": "vite --mode master",
    "dev:prod": "vite --mode prod",
    "build": "tsc -b && vite build",
    "build:master": "tsc -b tsconfig.prod.json && vite build --mode master",
    "build:prod": "tsc -b tsconfig.prod.json && vite build --mode prod",
    "lint": "eslint .",
    "preview": "vite preview",
    "validate-env": "node -e \"console.log('Environment variables loaded:', Object.keys(process.env).filter(k => k.startsWith('VITE_')).length)\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^12.2.1",
    "lucide-react": "^0.445.0",
    "nodemon": "^3.1.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "vite": "^7.1.7"
  }
}
