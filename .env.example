# Docker Environment Variables
# Copy this file to .env and update with your values

# Backend Configuration
PORT=8090
NODE_ENV=production

# Git Configuration
GIT_REPOSITORY_URL=git@github.com:nammayatri/configDashboard.git
GIT_BRANCH=main
GIT_REMOTE=origin
GIT_USER_NAME=Config Dashboard
GIT_USER_EMAIL=dhruv.singh@nammayatri.in
GIT_TOKEN=your_github_token_here
GIT_COMMIT_PREFIX=[Config Dashboard]
GIT_COMMIT_TEMPLATE={prefix} {action} {filename}.json
GIT_AUTO_COMMIT=true
GIT_AUTO_PUSH=true
GIT_VALIDATE_JSON=true
GIT_CREATE_BACKUP=false
GIT_MAX_FILE_SIZE=10MB

# Frontend Configuration
VITE_API_BASE_URL=http://localhost:8090/api
VITE_APP_ENVIRONMENT=development
VITE_APP_NAME=NEXUS Control System
VITE_APP_VERSION=2.0.0

# Firebase Configuration (if needed)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your_project.firebaseio.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id
